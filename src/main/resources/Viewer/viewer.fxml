<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>

<StackPane id="background" fx:id="main" prefHeight="480.0" prefWidth="640.0" stylesheets="@viewer.css" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Viewer.ViewerController">
   <children>
      <ImageView id="previous_button" fx:id="previous_button" cache="true" fitHeight="50.0" fitWidth="50.0" onMouseClicked="#onPreviousButtonClicked" pickOnBounds="true" preserveRatio="true" StackPane.alignment="CENTER_LEFT">
         <image>
            <Image url="@/assets/img/previous-image-white-gray.png" />
         </image>
         <StackPane.margin>
            <Insets left="10.0" />
         </StackPane.margin>
      </ImageView>
      <ImageView id="next_button" fx:id="next_button" cache="true" fitHeight="50.0" fitWidth="50.0" onMouseClicked="#onNextButtonClicked" pickOnBounds="true" preserveRatio="true" StackPane.alignment="CENTER_RIGHT">
         <image>
            <Image url="@/assets/img/next-image-white-gray.png" />
         </image>
         <StackPane.margin>
            <Insets right="10.0" />
         </StackPane.margin>
      </ImageView>
      <Button maxHeight="0.0" maxWidth="0.0" minHeight="0.0" minWidth="0.0" mnemonicParsing="false" onKeyPressed="#handleButtons" prefHeight="0.0" prefWidth="0.0" text="Button" />
   </children>
</StackPane>
